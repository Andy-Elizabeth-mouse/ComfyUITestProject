{
    "workflows": [
        {
            "name": "Basic Text-to-Image",
            "type": "text_to_image",
            "description": "Generate images from text descriptions using Stable Diffusion",
            "parameters": {
                "model": "v1-5-pruned-emaonly-fp16.safetensors",
                "width": 512,
                "height": 512,
                "steps": 20,
                "cfg_scale": 7.0,
                "sampler": "euler",
                "scheduler": "normal"
            },
            "template": "basic_txt2img.json"
        },
        {
            "name": "High Quality Text-to-Image",
            "type": "text_to_image",
            "description": "Generate high quality images with enhanced settings",
            "parameters": {
                "model": "realisticVisionV60_v60B1VAE.safetensors",
                "width": 768,
                "height": 768,
                "steps": 30,
                "cfg_scale": 7.0,
                "sampler": "dpmpp_2m",
                "scheduler": "karras"
            },
            "template": "high_quality_txt2img.json"
        },
        {
            "name": "Image-to-Image",
            "type": "image_to_image",
            "description": "Transform existing images based on text prompts",
            "parameters": {
                "model": "v1-5-pruned-emaonly-fp16.safetensors",
                "strength": 0.75,
                "steps": 20,
                "cfg_scale": 7.0,
                "sampler": "euler",
                "scheduler": "normal"
            },
            "template": "img2img.json"
        },
        {
            "name": "High-Resolution Upscale",
            "type": "upscale",
            "description": "Upscale images to higher resolution with AI enhancement",
            "parameters": {
                "upscale_model": "RealESRGAN_x4plus.pth",
                "scale_factor": 4,
                "face_enhance": true
            },
            "template": "upscale.json"
        },
        {
            "name": "Style Transfer",
            "type": "style_transfer",
            "description": "Apply artistic styles to images",
            "parameters": {
                "style_model": "style_model.safetensors",
                "style_strength": 0.8,
                "content_strength": 0.6
            },
            "template": "style_transfer.json"
        },
        {
            "name": "Mesh Texturing",
            "type": "mesh_texturing",
            "description": "Generate textures for existing 3D meshes using HunyuanI3D",
            "parameters": {
                "resolution": 768,
                "num_views": 10,
                "elevation": 3,
                "size": 1024,
                "do_rescale": false,
                "filename_prefix": "MV",
                "output_mesh_name": "TexturedMesh"
            },
            "template": "mesh_texturing.json"
        },
        {
            "name": "Image to 3D Full",
            "type": "image_to_3d",
            "description": "Generate complete 3D models with textures from single images using HunyuanI3D pipeline",
            "parameters": {
                "vae_model": "Hunyuan3D-vae-v2-1-fp16.ckpt",
                "dit_model": "hunyuan3d-dit-v2-1-fp16.ckpt",
                "steps": 25,
                "cfg": 7.5,
                "iso_value": 1.01,
                "resolution": 256,
                "max_faces": 200000,
                "algo": "mc",
                "post_process": true,
                "fix_normals": false,
                "remove_duplicated": true,
                "smooth": true,
                "unwrap": true,
                "simplify": false,
                "mv_resolution": 768,
                "num_views": 10,
                "elevation": 3,
                "size": 1024,
                "do_rescale": false,
                "format": "glb",
                "save_texture": true,
                "output_mesh_name": "Generated3D"
            },
            "template": "image_to_3d_full.json"
        }
    ],
    "server_settings": {
        "default_url": "http://192.168.2.169:8188",
        "timeout": 300,
        "max_retries": 3,
        "poll_interval": 2.0
    },
    "ui_settings": {
        "default_workflow": "Basic Text-to-Image",
        "auto_save": true,
        "preview_size": "medium",
        "show_advanced_options": false
    }
}
