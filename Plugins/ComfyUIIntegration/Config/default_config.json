{
    "workflows": [
        {
            "name": "Basic Text-to-Image",
            "type": "text_to_image",
            "description": "Generate images from text descriptions using Stable Diffusion",
            "parameters": {
                "model": "v1-5-pruned-emaonly-fp16.safetensors",
                "width": 512,
                "height": 512,
                "steps": 20,
                "cfg_scale": 7.0,
                "sampler": "euler",
                "scheduler": "normal"
            },
            "template": "basic_txt2img.json"
        },
        {
            "name": "High Quality Text-to-Image",
            "type": "text_to_image",
            "description": "Generate high quality images with enhanced settings",
            "parameters": {
                "model": "realisticVisionV60_v60B1VAE.safetensors",
                "width": 768,
                "height": 768,
                "steps": 30,
                "cfg_scale": 7.0,
                "sampler": "dpmpp_2m",
                "scheduler": "karras"
            },
            "template": "high_quality_txt2img.json"
        },
        {
            "name": "Image-to-Image",
            "type": "image_to_image",
            "description": "Transform existing images based on text prompts",
            "parameters": {
                "model": "v1-5-pruned-emaonly-fp16.safetensors",
                "strength": 0.75,
                "steps": 20,
                "cfg_scale": 7.0,
                "sampler": "euler",
                "scheduler": "normal"
            },
            "template": "img2img.json"
        },
        {
            "name": "Text to 3D Model",
            "type": "text_to_3d",
            "description": "Generate 3D models from text descriptions",
            "parameters": {
                "guidance_scale": 7.5,
                "num_inference_steps": 50,
                "output_format": "glb",
                "texture_resolution": 1024,
                "generate_textures": true
            },
            "template": "text_to_3d.json"
        },
        {
            "name": "Image to 3D Model",
            "type": "image_to_3d",
            "description": "Generate 3D models from input images",
            "parameters": {
                "strength": 0.8,
                "guidance_scale": 7.5,
                "num_inference_steps": 40,
                "output_format": "glb",
                "texture_size": 1024,
                "mesh_resolution": "high",
                "optimize_for_game": true,
                "generate_lods": true
            },
            "template": "image_to_3d.json"
        },
        {
            "name": "PBR Texture Generation",
            "type": "texture_generation",
            "description": "Generate complete PBR texture sets (diffuse, normal, roughness, metallic, AO)",
            "parameters": {
                "width": 1024,
                "height": 1024,
                "guidance_scale": 7.5,
                "num_inference_steps": 30,
                "generate_normal": true,
                "generate_roughness": true,
                "generate_metallic": true,
                "generate_ao": true,
                "seamless": true,
                "format": "png",
                "quality": 95
            },
            "template": "texture_generation.json"
        },
        {
            "name": "High-Resolution Upscale",
            "type": "upscale",
            "description": "Upscale images to higher resolution with AI enhancement",
            "parameters": {
                "upscale_model": "RealESRGAN_x4plus.pth",
                "scale_factor": 4,
                "face_enhance": true
            },
            "template": "upscale.json"
        },
        {
            "name": "Style Transfer",
            "type": "style_transfer",
            "description": "Apply artistic styles to images",
            "parameters": {
                "style_model": "style_model.safetensors",
                "style_strength": 0.8,
                "content_strength": 0.6
            },
            "template": "style_transfer.json"
        }
    ],
    "server_settings": {
        "default_url": "http://127.0.0.1:8188",
        "timeout": 300,
        "max_retries": 3,
        "poll_interval": 2.0
    },
    "ui_settings": {
        "default_workflow": "Basic Text-to-Image",
        "auto_save": true,
        "preview_size": "medium",
        "show_advanced_options": false
    }
}
