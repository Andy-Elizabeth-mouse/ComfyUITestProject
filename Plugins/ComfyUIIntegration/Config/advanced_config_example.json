{
    "workflows": [
        {
            "name": "Basic Text-to-Image",
            "type": "text_to_image",
            "description": "Generate images from text descriptions using Stable Diffusion",
            "parameters": {
                "model": "v1-5-pruned-emaonly-fp16.safetensors",
                "width": 512,
                "height": 512,
                "steps": 20,
                "cfg_scale": 7.0,
                "sampler": "euler",
                "scheduler": "normal"
            },
            "template": "basic_txt2img.json"
        },
        {
            "name": "High Quality Text-to-Image",
            "type": "text_to_image",
            "description": "Generate high quality images with enhanced settings",
            "parameters": {
                "model": "realisticVisionV60_v60B1VAE.safetensors",
                "width": 768,
                "height": 768,
                "steps": 30,
                "cfg_scale": 7.0,
                "sampler": "dpmpp_2m",
                "scheduler": "karras"
            },
            "template": "high_quality_txt2img.json"
        },
        {
            "name": "Image to Image",
            "type": "image_to_image",
            "description": "Transform existing images using AI guidance",
            "parameters": {
                "model": "v1-5-pruned-emaonly-fp16.safetensors",
                "strength": 0.75,
                "steps": 20,
                "cfg_scale": 7.0,
                "sampler": "euler",
                "scheduler": "normal"
            },
            "template": "img2img.json"
        },
        {
            "name": "Text to 3D",
            "type": "text_to_3d",
            "description": "Generate 3D models from text descriptions",
            "parameters": {
                "model": "sv3d_u.safetensors",
                "steps": 25,
                "cfg_scale": 7.5,
                "sampler": "dpmpp_2m",
                "scheduler": "karras",
                "num_views": 21,
                "output_format": "obj",
                "elevation_range": [-15, 15],
                "azimuth_step": 17.14,
                "mesh_resolution": 256,
                "texture_resolution": 512
            },
            "template": "text_to_3d.json",
            "advanced_settings": {
                "3d_reconstruction": {
                    "method": "neural_radiance_fields",
                    "depth_estimation": "midas",
                    "surface_extraction": "marching_cubes",
                    "mesh_decimation": 0.1,
                    "smooth_normals": true
                },
                "optimization": {
                    "vertex_optimization": true,
                    "uv_unwrapping": "angle_based",
                    "texture_baking": true,
                    "lod_generation": false
                }
            }
        },
        {
            "name": "Image to 3D",
            "type": "image_to_3d",
            "description": "Generate 3D models from 2D images using depth estimation",
            "parameters": {
                "depth_model": "DPT-Hybrid-MiDaS",
                "reconstruction_method": "poisson",
                "depth_scale": 0.1,
                "smooth_iterations": 3,
                "uv_method": "smart_projection",
                "output_format": "obj",
                "max_faces": 10000,
                "texture_resolution": 1024
            },
            "template": "image_to_3d.json",
            "advanced_settings": {
                "depth_processing": {
                    "depth_smoothing": true,
                    "edge_preservation": true,
                    "hole_filling": true,
                    "normal_estimation": "plane_fitting"
                },
                "mesh_generation": {
                    "adaptive_resolution": true,
                    "feature_preservation": true,
                    "boundary_smoothing": true,
                    "self_intersection_removal": true
                },
                "texture_processing": {
                    "seam_reduction": true,
                    "texture_atlas_optimization": true,
                    "color_correction": true,
                    "detail_enhancement": false
                }
            }
        },
        {
            "name": "Texture Generation",
            "type": "texture_generation",
            "description": "Generate seamless PBR textures for 3D materials",
            "parameters": {
                "model": "materialDiffusion_v10.safetensors",
                "resolution": 1024,
                "steps": 30,
                "cfg_scale": 8.0,
                "sampler": "dpmpp_sde",
                "scheduler": "karras",
                "tile_size": 512,
                "overlap": 64,
                "generate_pbr_maps": true,
                "normal_strength": 1.0,
                "height_scale": 0.05
            },
            "template": "texture_generation.json",
            "pbr_settings": {
                "diffuse": {
                    "enabled": true,
                    "format": "RGBA8",
                    "compression": "DXT1",
                    "srgb": true,
                    "quality": "high"
                },
                "normal": {
                    "enabled": true,
                    "format": "BC5",
                    "compression": "BC5",
                    "srgb": false,
                    "strength_multiplier": 1.0,
                    "flip_green_channel": false
                },
                "roughness": {
                    "enabled": true,
                    "format": "G8",
                    "compression": "BC4",
                    "srgb": false,
                    "invert_values": false,
                    "contrast_adjustment": 1.0
                },
                "metallic": {
                    "enabled": true,
                    "format": "G8",
                    "compression": "BC4",
                    "srgb": false,
                    "threshold": 0.5,
                    "edge_softening": true
                },
                "height": {
                    "enabled": true,
                    "format": "R16",
                    "compression": "BC4",
                    "srgb": false,
                    "scale_factor": 0.05,
                    "detail_scale": 1.0
                },
                "ao": {
                    "enabled": false,
                    "format": "G8",
                    "compression": "BC4",
                    "srgb": false,
                    "intensity": 1.0,
                    "radius": 0.1
                }
            },
            "post_processing": {
                "seamless_tiling": {
                    "method": "offset_filter",
                    "edge_blending": 0.1,
                    "corner_blending": 0.05
                },
                "detail_enhancement": {
                    "sharpen_diffuse": false,
                    "enhance_normal": true,
                    "contrast_boost": 1.1
                },
                "optimization": {
                    "mipmaps": true,
                    "compression_quality": "high",
                    "memory_optimization": true
                }
            }
        },
        {
            "name": "Advanced PBR Material",
            "type": "texture_generation",
            "description": "Generate complete PBR material with advanced features",
            "parameters": {
                "model": "materialDiffusion_v10.safetensors",
                "resolution": 2048,
                "steps": 40,
                "cfg_scale": 9.0,
                "sampler": "dpmpp_2m_sde",
                "scheduler": "karras",
                "tile_size": 1024,
                "overlap": 128,
                "generate_pbr_maps": true,
                "normal_strength": 1.2,
                "height_scale": 0.08
            },
            "template": "texture_generation.json",
            "material_features": {
                "subsurface_scattering": {
                    "enabled": false,
                    "radius": 0.1,
                    "falloff": [1.0, 0.2, 0.1]
                },
                "clearcoat": {
                    "enabled": false,
                    "strength": 0.5,
                    "roughness": 0.1
                },
                "emission": {
                    "enabled": false,
                    "intensity": 1.0,
                    "color": [1.0, 1.0, 1.0]
                },
                "anisotropy": {
                    "enabled": false,
                    "strength": 0.0,
                    "direction": 0.0
                }
            }
        }
    ],
    "server_settings": {
        "default_url": "http://127.0.0.1:8188",
        "timeout": 300,
        "max_retries": 3,
        "poll_interval": 2.0,
        "connection_pool_size": 5,
        "request_timeout": 60,
        "download_timeout": 120,
        "upload_chunk_size": 8192
    },
    "ui_settings": {
        "default_workflow": "Basic Text-to-Image",
        "auto_save": true,
        "preview_size": "medium",
        "show_advanced_options": false,
        "enable_progress_notifications": true,
        "auto_refresh_workflows": true,
        "default_save_location": "/Game/ComfyUI/Generated"
    },
    "performance_settings": {
        "max_concurrent_generations": 2,
        "memory_limit_mb": 4096,
        "texture_streaming": true,
        "mesh_lod_generation": true,
        "async_processing": true,
        "cache_generated_assets": true,
        "cleanup_temp_files": true
    },
    "output_settings": {
        "3d_models": {
            "default_format": "obj",
            "include_materials": true,
            "include_textures": true,
            "compression": false,
            "coordinate_system": "unreal",
            "unit_scale": 100.0
        },
        "textures": {
            "default_format": "png",
            "quality": 95,
            "enable_compression": true,
            "generate_mipmaps": true,
            "power_of_two_resize": true
        },
        "materials": {
            "create_material_instances": true,
            "auto_assign_textures": true,
            "use_pbr_workflow": true,
            "create_master_material": false
        }
    },
    "integration_settings": {
        "content_browser": {
            "auto_refresh": true,
            "organize_by_type": true,
            "create_folder_structure": true,
            "name_prefix": "ComfyUI_"
        },
        "level_editor": {
            "auto_place_generated_models": false,
            "default_placement_location": [0, 0, 0],
            "auto_assign_materials": true
        },
        "material_editor": {
            "auto_open_generated_materials": false,
            "show_parameter_groups": true,
            "enable_real_time_preview": true
        }
    },
    "debug_settings": {
        "enable_logging": true,
        "log_level": "info",
        "save_intermediate_results": false,
        "profile_performance": false,
        "verbose_error_messages": true,
        "dump_workflow_json": false
    }
}
