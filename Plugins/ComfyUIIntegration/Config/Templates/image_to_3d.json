{
    "1": {
        "inputs": {
            "image": "{INPUT_IMAGE}"
        },
        "class_type": "LoadImage"
    },
    "2": {
        "inputs": {
            "images": ["1", 0],
            "width": 512,
            "height": 512,
            "crop": "center"
        },
        "class_type": "ImageScale"
    },
    "3": {
        "inputs": {
            "image": ["2", 0],
            "depth_model": "DPT-Hybrid-MiDaS",
            "compute_normal": true
        },
        "class_type": "DepthEstimation"
    },
    "4": {
        "inputs": {
            "image": ["2", 0],
            "depth": ["3", 0],
            "normal": ["3", 1],
            "method": "poisson",
            "depth_scale": 0.1,
            "smooth_iterations": 3
        },
        "class_type": "ImageTo3DMesh"
    },
    "5": {
        "inputs": {
            "mesh": ["4", 0],
            "texture": ["2", 0],
            "uv_method": "smart_projection"
        },
        "class_type": "ApplyTexture"
    },
    "6": {
        "inputs": {
            "filename_prefix": "image_to_3d",
            "mesh_data": ["5", 0],
            "format": "obj"
        },
        "class_type": "Save3DModel"
    }
}